#import "YAAddressInfo.h"

@implementation YAAddressInfo

- (NSString*) handle
{
    // I wish Objective-C had the concept of an abstract base class...
    // We don't generally like to use exceptions, but it's appropriate for a design/development (rather than "runtime") error like this.
    [NSException raise:@"unexpected invocation" format:@"Expected subclass to implement"];
    return nil;
}


@end
