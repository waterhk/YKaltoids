#import "YAEmailAddressInfo.h"

@implementation YAEmailAddressInfo
@synthesize handle = handle_;

- (id) initWithNormalizedEmailAddress:(NSString *)normalizedEmailAddress
{
    self = [super init];
    if(self)
    {
        handle_ = normalizedEmailAddress;
    }
    
    return self;
}

@end
